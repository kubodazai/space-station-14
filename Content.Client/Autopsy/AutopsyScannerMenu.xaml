<DefaultWindow xmlns="https://spacestation14.io"
            Title="{Loc 'autopsy-scanner-interface-title'}"
            MinSize="350 200"
            SetSize="350 500">
    <BoxContainer Orientation="Vertical">
        <BoxContainer Orientation="Horizontal">
            <Button Name="Print"
                TextAlign="Center"
                HorizontalExpand="True"
                Access="Public"
                Disabled="True"
                Text="{Loc 'autopsy-scanner-interface-print'}" />
            <Button Name="Clear"
                TextAlign="Center"
                HorizontalExpand="True"
                Access="Public"
                Disabled="True"
                Text="{Loc 'autopsy-scanner-interface-clear'}" />
        </BoxContainer>
        <Label
            Name="NameLabel"
            Align="Center" />
        <Label
            Name="Diagnostics"
            Text="{Loc autopsy-scanner-interface-no-data}"/>
    </BoxContainer>
</DefaultWindow>
